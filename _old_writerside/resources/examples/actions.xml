				</layout>
			</dataRef>
		</dataGroup>
		<event type="finish">
			<id>t4_finish</id>
			<actions phase="pre">
				<action>text_2: f.text_2;
					change text_2 value { "Conclusion made."; }
				</action>
			</actions>
			<actions phase="post">
				<action>t9: t.t9,
					text_1: f.text_1;
					make text_1, visible on t9 when { true }
				</action>
			</actions>
		</event>
